{
    "description": "US Census Synthetic people",
    "background": "Black",
    "data": {"url": "census.snappy_data.json"},
    "derived_buffers": {
        "all": {"buffers": [], "aggregate": "sum"}
    },
    "reencoding": {
        "label": {
            "field": "z",
            "type": "nominal",
            "scale": {
                "domain": ["w", "h", "a", "b", "o"],
                "range": ["White", "Hispanic", "Asian", "Black", "Other"]
            }
        },
        "color": {
            "field": "z",
            "type": "nominal",
            "scale": {
                "range": ["Blue", "Orange", "Red", "Green", "Brown"],
                "scale": "log"
            }
        }
    },
    "rebin": {},
    "compose": {
        "mix": "blend",
        "mixing": "additive"
    }
}
